package day5.com.capg;

public class Myarr {
	public static void main(String[] args) {
		int array[]= {25,12,4,45,8};
		int max =array[0];
		for(int i=0;i<array.length;i++){
			if(array[i]>max) {
				max= array[i];
				System.out.println("the largest number is"+max);
				
				
			}
		

			
	

	
		}
	}
}
		
		
	

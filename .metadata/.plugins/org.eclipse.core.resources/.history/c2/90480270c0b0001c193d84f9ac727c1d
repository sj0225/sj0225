package day5.com.capg;

public class Myarr {

	public static void main(String[] args) {
		int temp=0;
		int size=0;
		int array[]= {25,12,34,45,8};
		size=array.length;
		for(int i=0;i<size;i++){
			for(int j=i;j<size;j++) {
				if(array[i]>array[j]) {
					temp=array[i];
					array[i]=array[j];
					array[j]=temp;
				}
			}
			
		}
	System.out.println("the largest number is"+size);
	{
		
	}
		
		

	}

}

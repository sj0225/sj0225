package com.capg.day7;


class Mythred implements Runnable
{

	@Override
	public void run() {
		for(int i=0;i<10;i++) {
			System.out.println("i "+i);
		}
		
	}
	
}

public class Myth {
	public static void main(String[] args) {
		Mythred s1=new Mythred();
		s1.run();

		Mythred s2=new Mythred();
		s2.run();
		
		
	}

}
